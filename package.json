    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.5",
    "body-parser": "^1.19.0",
    "config": "^3.3.3",
